@{
    ViewBag.Title = "Index";
}
<table>
    <thead>
        <tr>
            <th>Number @Html.ActionLink("+", "OrderTasks", "Home", new { prop = "Id" }, null)</th>
            <th>Userid @Html.ActionLink("+", "OrderTasks", "Home", new { prop = "UserId" }, null)</th>
            <th>Description @Html.ActionLink("+", "OrderTasks", "Home", new { prop = "Description" }, null)</th>
            <th>Data @Html.ActionLink("+", "OrderTasks", "Home", new { prop = "Data" }, null)</th>
            <th>Priority @Html.ActionLink("+", "OrderTasks", "Home", new { prop = "Priority" }, null)</th>
            <th>Completed @Html.ActionLink("+", "OrderTasks", "Home", new { prop = "IsCompleted" }, null)</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var t in ViewBag.Tasks)
        {
            <tr>
                <td>@t.Id</td>
                <td>@t.UserId</td>
                <td>@t.Description</td>
                <td>@t.Data</td>
                <td>@t.Priority</td>
                <td>@t.IsComplete</td>
            </tr>
        }
    </tbody>
</table>