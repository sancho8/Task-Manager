<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Doer Task-Manager</title>
    <link href="~/Content/calendar-widget/css/responsive-calendar.css" rel="stylesheet" media="screen">
    <link rel="icon" type="image/x-icon" href="~/Content/favicon.ico">
    <link href="~/Content/bootstrap.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/Reset.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
    <script src="~/scripts/jquery-1.10.2.js"></script>
    <script src="~/Content/calendar-widget/js/responsive-calendar.min.js"></script>
</head>
<body>
    @Html.Partial("LoginWindow")
    @Html.Partial("RegistrationWindow")
    @Html.Partial("ExitWindow")
    @if (IsSectionDefined("Header"))
    {
    @RenderSection("Header")
    }
    @RenderBody()
    @if (IsSectionDefined("Footer"))
    {
    @RenderSection("Footer")
    }
    <script src="~/scripts/jquery.json-2.2.min.js"></script>
    <script src="~/scripts/formchecker.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/scripts/jquery.unobtrusive-ajax.js"></script>
</body>
</html>